<section class="list">
  <h1><%= @username.capitalize %>'s Contact List</h1>

  <a href="/new" class="link">New Contact</a>
  <% if @contact_list %>
    <ul>
      <% @contact_list.each do |id, contact_info| %>
      <li><a href="/<%= id %>"><%= contact_info[:name] %></a></li>
      <% end %>
    </ul>
  <% end %>
</section>